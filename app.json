{
  "expo": {
    "name": "Minote",
    "slug": "minote",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0AC2B7"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/foregroundImage.png",
        "backgroundImage": "./assets/backgroundImage.png"
      },
      "icon": "./assets/icon.png",
      "package": "jmstdy95.mynote",
      "permissions": [],
      "versionCode": 1
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A simple note-taking app. All notes will be saved in phone memory (AsyncStorage) and currently, user can only type title and text in the note. More options will come in the future."
  }
}
